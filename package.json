{
    "name": "wagtail-landing-page",
    "version": "1.0.0",
    "description": "A flexible landing page application built with Wagtail CMS",
    "scripts": {
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "babel-jest": "^29.7.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0"
    },
    "jest": {
        "setupFiles": [
            "./landing/static/js/tests/setup.js"
        ],
        "testEnvironment": "jsdom",
        "moduleFileExtensions": [
            "js"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "testMatch": [
            "**/tests/test_*.js",
            "**/tests/**/*.test.js"
        ]
    },
    "dependencies": {
        "wagtail-landing-page": "file:"
    }
}
